%Part B: create squre of size 11 and disk of radius 6
s11 = zeros(11,11);
s11(:)=255;

c6 = zeros(11,11)
for c=1:11
    y_lower_bound = round(6-sqrt(36-(c-6)^2));
    y_upper_bound = round(6+sqrt(36-(c-6)^2));
    for r=y_lower_bound+1:y_upper_bound-1
        c6(r,c)=255;
    end
end
c6
imshow(c6)